{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "tweets-analysis/{userId}"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "db",
      "databaseName": "tweetsAnalysisDatabase",
      "collectionName": "tweetReports",
      "connectionStringSetting": "AzureCosmosDBConnectionString",
      "createIfNotExists": "true",
      "partitionKey": "{userId}"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "reports",
      "databaseName": "tweetsAnalysisDatabase",
      "collectionName": "tweetReports",
      "connectionStringSetting": "AzureCosmosDBConnectionString",
      "sqlQuery": "SELECT * from c where c.user_id = {userId}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}